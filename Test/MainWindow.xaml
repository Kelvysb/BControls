<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test"
        mc:Ignorable="d"
        Title="Test" Height="200" Width="300" WindowStyle="SingleBorderWindow" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <Button Name="btnOk" Content="Ok" Margin="5"/>
            <Button Name="btnOkCancel" Content="Ok Cancel" Margin="5"/>
            <Button Name="btnYesNo" Content="Yes No" Margin="5"/>
            <Button Name="btnYesNoCancel" Content="Yes No Cancel" Margin="5"/>
            <Button Name="btnError" Content="Error" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
